{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "components/*": ["src/components/*"],
      "assets/*": ["assets/*"]
      // "providers/*": ["src/providers/*"]
      // "lib/*": ["src/lib/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/components/folder"
  ]
}
